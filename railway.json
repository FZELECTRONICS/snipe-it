{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "dockerfile",
    "dockerfile": "Dockerfile"
  },
  "deploy": {
    "numReplicas": 1,
    "startCommand": "/startup.sh",
    "healthchecks": {
      "readiness": {
        "httpGet": {
          "path": "/",
          "port": 8000
        },
        "initialDelaySeconds": 30,
        "periodSeconds": 10
      }
    }
  }
}
